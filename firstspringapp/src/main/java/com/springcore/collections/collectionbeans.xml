<?xml version="1.0" encoding="UTF-8"?>
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
                http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
    
    <bean name = "emp1" class = "com.springcore.collections.Emp">
    	<property name ="name" value ="Pooja Tikhe"> </property>
    	
    	<property name = "phones" >
    		<list>
    			<value> 101 </value>
    			<value> 102 </value>
    			<value> 103 </value>
    		</list>
    	</property>
    	
    	<property name = "addresses">
    		<set>
    			<value> Pune </value>
    			<value> Mumbai </value>
    			<value> Ahmadnagar </value>
    		</set>
    	</property>
    	
    	<property name = "courses">
    		<map>
    			<entry key = "c++" value = "1 month"> </entry>
    			<entry key = "java" value = "2 month"></entry>
    			<entry key = "python" value = "3 month"></entry>
    		</map>
    	</property>
    	
    </bean>
    
    
    
    
    
    
    
</beans>